{{alias}}
+crunch-all-cookies = +crunch-incoming-cookies +crunch-outgoing-cookies
-crunch-all-cookies = -crunch-incoming-cookies -crunch-outgoing-cookies
 allow-all-cookies  = -crunch-all-cookies -session-cookies-only -filter{content-cookies}
 allow-popups       = -filter{all-popups} -filter{unsolicited-popups}
+block-as-image     = +block{Blocked image request.} +handle-as-image
-block-as-image     = -block

block     = +block-as-image +handle-as-empty-document
shop      = -crunch-all-cookies allow-popups
auto-https = +redirect{s@http://@https://@}

polipo = +forward-override{forward 127.0.0.1:8123} allow-all-cookies
polipo-direct = +forward-override{forward 127.0.0.1:8456} allow-all-cookies
goagent = +forward-override{forward 127.0.0.1:8087} allow-all-cookies
tor = +forward-override{forward-sock5 127.0.0.1:9050 .}
direct = +forward-override{forward .} -block
local = +forward-override{forward .} -block allow-popups allow-all-cookies
default = +forward-override{forward .}

